@model DirectoryListApp.Areas.Directory.Models.DirectoryModel
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">

    @using (Html.BeginForm("Create", "DirectoryItem", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        Html.RenderPartial("VUC_Add");
        <br />
        <div class="clearfix"></div>

        <input type="submit" value="Save" class="btn btn-success" />
        <div class="btn btn-defult"> @Html.ActionLink("Back To List", "Index")</div>

    }
</div>


